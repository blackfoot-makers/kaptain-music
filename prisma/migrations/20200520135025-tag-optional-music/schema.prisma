datasource sqlite {
  provider = "sqlite"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Tag {
  id    Int     @default(autoincrement()) @id
  name  String
  music Int?
  Music Music[]
}

model Music {
  id     Int    @default(autoincrement()) @id
  name   String
  author String
  tags   Tag    @relation(fields: [id], references: [id])
}
